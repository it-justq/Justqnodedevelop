<!---------------------------------------------------------------->
<!--MAIN CONTAINER-->
<div class="container-width-content content-subheader">
    <p id="container-titulo">Modificación Envío: {{data.datos.envio_transporte_referencia}}<a
            href="/clientes/envios">Finalizar <i class="bi bi-arrow-left-circle"></i></a></p>
    <div class="contentData">
        <p class="contentDataContainerTitle">Información general</p>
        <div class="contentDataContainer">
            <label>Referencia Transporte</label>
            <input type="text"  placeholder="Referencia del transporte" value="{{valorNull data.datos.envio_transporte_referencia}}" onchange="uploadDatos('envio_transporte_referencia','{{envio_id}}', this.value)">
            <i class="bi bi-check-lg tick-confirm" id="tick-confirm-envio_transporte_referencia"></i>
        </div>
        <div class="contentDataContainer">
            <label>Referencia Pedido</label>
            <input type="text"  placeholder="Referencia del pedido" value="{{valorNull data.datos.envio_pedido_referencia}}" onchange="uploadDatos('envio_pedido_referencia','{{envio_id}}', this.value)">
            <i class="bi bi-check-lg tick-confirm" id="tick-confirm-envio_pedido_referencia"></i>
        </div>
        <div class="contentDataContainer">
            <label>Fecha de carga</label>
            <input type="date"value="{{valorNull data.datos.envio_carga_fecha}}" onchange="uploadDatos('envio_carga_fecha','{{envio_id}}', this.value)">
            <i class="bi bi-check-lg tick-confirm" id="tick-confirm-envio_carga_fecha"></i>
        </div>
        <div class="contentDataContainer">
            <label>Producto</label>
            <input type="text"  placeholder="Producto" value="{{valorNull data.datos.envio_producto_nombre}}" readonly>

            <!--input type="text"  placeholder="Producto" value="{{valorNull data.datos.envio_producto_nombre}}" onchange="uploadDatos('envio_producto','{{envio_id}}', this.value)">
            <i class="bi bi-check-lg tick-confirm" id="tick-confirm-envio_producto"></i-->
        </div>
        <div class="contentDataContainer">
            <label>Proveedor</label>
            <input type="text"  placeholder="Proveedor" value="{{valorNull data.datos.envio_proveedor}}" onchange="uploadDatos('envio_proveedor','{{envio_id}}', this.value)">
            <i class="bi bi-check-lg tick-confirm" id="tick-confirm-envio_proveedor"></i>
        </div>
        <div class="contentDataContainer">
            <label>Destinatario</label>
            <input type="text"  placeholder="Destinatario" value="{{valorNull data.datos.envio_consignatario}}" onchange="uploadDatos('envio_consignatario','{{envio_id}}', this.value)">
            <i class="bi bi-check-lg tick-confirm" id="tick-confirm-envio_consignatario"></i>
        </div>
        <div class="contentDataContainer">
            <label>Pais destino</label>
            <input type="text"  placeholder="Pais" value="{{valorNull data.datos.envio_pais_destino_nombre}}" readonly>

            <!--input type="text"  placeholder="Pais destino" value="{{valorNull data.datos.envio_pais_destino_nombre}}" onchange="uploadDatos('envio_pais_destino_nombre','{{envio_id}}', this.value)">
            <i class="bi bi-check-lg tick-confirm" id="tick-confirm-envio_pais_destino_nombre"></i-->
        </div>
    </div>

    <div class="contentData">
        <p class="contentDataContainerTitle">Lugar de carga</p>
        <div class="contentDataContainer">
            <label>Almacén de carga</label>
            <input type="text"  placeholder="Almacén" value="{{valorNull data.datos.envio_carga_almacen}}" onchange="uploadDatos('envio_carga_almacen','{{envio_id}}', this.value)">
